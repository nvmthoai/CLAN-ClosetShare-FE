# Example environment variables for Vite app
# Copy to .env and fill with your values

# Public app metadata
VITE_APP_URL=https://closetshare.vercel.app
VITE_APP_NAME=ClosetShare
VITE_APP_DESCRIPTION="Your Virtual Wardrobe - Social Fashion Platform"

# API Base URL (used by axios fetcher)
# Either VITE_BASE_URL or VITE_API_BASE_URL can be set; VITE_BASE_URL takes precedence
VITE_BASE_URL=http://localhost:3000
# VITE_API_BASE_URL=
